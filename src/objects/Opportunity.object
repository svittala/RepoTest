        <externalId>false</externalId>
        <formula>AND(ISBLANK(&quot;&quot;),ISBLANK( EMEA_SAP_Required_Data_Checklist__c ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
