    <validationRules>
        <fullName>Approved_primary_quote</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 

 IsWon = true ,
 
 NOT(Primary_Quote_approved__c )
)</errorConditionFormula>
        <errorMessage>Your Primary quote must be approved to move this record to WON</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Associated_Winning_Opportunity</fullName>
        <active>true</active>
        <description>This field should only be edited and therefore required if the Win/Loss reason is &quot;Won with Another Opportunity&quot;</description>
        <errorConditionFormula>AND(
NOT(CONTAINS($Profile.Name, &quot;EMEA&quot;)),
NOT(CONTAINS($Profile.Name, &quot;Administrator&quot;)),
NOT(CONTAINS($Profile.Name, &quot;Integration&quot;)), 
OR(
AND(
ISPICKVAL(Win_Loss_Reason__c, &quot;Won with Another Opportunity&quot;), 
ISBLANK(Associated_Winning_Opportunity__c)
),
AND(
NOT(ISPICKVAL(Win_Loss_Reason__c, &quot;Won with Another Opportunity&quot;)),
NOT(ISBLANK(Associated_Winning_Opportunity__c))
)
)
)</errorConditionFormula>
        <errorDisplayField>Associated_Winning_Opportunity__c</errorDisplayField>
        <errorMessage>This field should only be edited and therefore required if the Win/Loss reason is &quot;Won with Another Opportunity&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Lost_Competitor</fullName>
        <active>true</active>
        <description>Must add the competitor when an opportunity is set to closed/lost</description>
        <errorConditionFormula>AND( ISPICKVAL(StageName, &apos;Closed Lost&apos;), 
NOT(ISPICKVAL(SBU__c , &apos;EAS&apos;)),
NOT(ISPICKVAL(Win_Loss_Reason__c, &apos;Won with Another Opportunity&apos;)),  
ISBLANK(Competitor_Lost_To__c) )</errorConditionFormula>
        <errorDisplayField>Competitor_Lost_To__c</errorDisplayField>
        <errorMessage>Please select.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Reason</fullName>
        <active>true</active>
        <description>Select a closed lost or won reason</description>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(StageName, &apos;Closed Won&apos;),
ISPICKVAL(StageName, &apos;Closed Lost&apos;)
), 
ISBLANK(TEXT(Win_Loss_Reason__c))
)</errorConditionFormula>
        <errorDisplayField>Win_Loss_Reason__c</errorDisplayField>
        <errorMessage>Please select.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Opp_Creation_on_National_Acc</fullName>
        <active>true</active>
        <description>Prevent creation of Opportunities on Accounts flagged as National, excluding some roles and Profiles.</description>
        <errorConditionFormula>AND(
Account.National__c,
AND(
$Profile.Name &lt;&gt; &apos;System Administrator&apos;,
$Profile.Name &lt;&gt; &apos;EnerSys Integration&apos;,
$Profile.Name &lt;&gt; &apos;EnerSys System Administrator&apos;,
$UserRole.Name &lt;&gt; &apos;EnerSys Delaware Inc.&apos;,
$UserRole.Name &lt;&gt; &apos;Worldwide&apos;,
$UserRole.Name &lt;&gt; &apos;MP - Americas NA Support&apos;,
$UserRole.Name &lt;&gt; &apos;MP - Americas&apos;,
$UserRole.Name &lt;&gt; &apos;RP - Americas NA Support&apos;,
$UserRole.Name &lt;&gt; &apos;RP - Americas&apos;
)
)</errorConditionFormula>
        <errorMessage>You cannot create an Opportunity on a Account flagged as National.</errorMessage>
    </validationRules>
