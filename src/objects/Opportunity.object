    <fields>
        <fullName>EMEA_SAP_Required_Data_Checklist__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK( Account.BillingCity ), &quot;Please add a BillingCity to the Sold To Account; &quot;, &quot;&quot;)
&amp;
IF(ISBLANK( Account.Customer_Language__r.Code__c ), &quot;Please add a Language to the Sold To Account; &quot;, &quot;&quot;)
&amp;
IF(Sales_Area_Data__c==NULL, &quot;Please add Sales Area Data; &quot;, &quot;&quot;)
&amp;
IF(Sales_Area_Data__r.Sales_Area_Sales_Organisation_Code__c==NULL, &quot;Please add a Sales Area on Sales Area Data&quot;, &quot;&quot;)
&amp;
IF (ISBLANK(TEXT(Sales_Area_Data__r.CurrencyIsoCode)), &quot;Please add a Currency on Sales Area Data; &quot;, &quot;&quot;)
&amp;
IF (ISBLANK(Sales_Area_Data__r.Customer_Group_lookup__r.Code__c), &quot;Please add a Customer Group on Sales Area Data; &quot;, &quot;&quot;)
&amp;
IF (ISBLANK(Sales_Area_Data__r.Delivery_Priority__r.Code__c), &quot;Please add a Delivery Priority on Sales Area Data; &quot;, &quot;&quot;)
&amp;
IF (ISBLANK(Sales_Area_Data__r.Incoterm_Code_lookup__r.Code__c), &quot;Please add an Incoterm Code on Sales Area Data; &quot;, &quot;&quot;)
&amp;
IF (ISBLANK(Sales_Area_Data__r.Sales_District__r.Code__c), &quot;Please add a Sales District on Sales Area Data; &quot;, &quot;&quot;)
&amp;
IF (ISBLANK(Sales_Area_Data__r.Sales_Office_lookup__r.Code__c), &quot;Please add a Sales Office on Sales Area Data; &quot;, &quot;&quot;)
&amp;
IF (ISBLANK(Sales_Area_Data__r.Shipping_Conditions__r.Code__c), &quot;Please add a Shipping Conditions on Sales Area Data; &quot;, &quot;&quot;)
&amp;
IF (ISBLANK(Sales_Area_Data__r.Terms_of_Payment_lookup__r.Code__c), &quot;Please add Terms of Payment on Sales Area Data; &quot;, &quot;&quot;)</formula>
        <label>EMEA SAP Required Data Checklist</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
