    <validationRules>
        <fullName>Validate_the_Key</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				NOT(
								Key__c =
        &quot;B&quot; 
        &amp; &quot;-&quot; &amp;    
        RIGHT(TEXT(Code_Type__c), 2)
        &amp; &quot;-&quot; &amp;
        Code__c
				),
    NOT(
